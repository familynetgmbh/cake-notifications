{
    "name": "codekanzlei/cake-notifications",
    "description": "CakePHP3 Notifications Plugin",
    "license": "MIT",
    "type": "cakephp-plugin",
    "extra": {
        "installer-name": "Notifications"
    },
    "require": {
        "cakephp/plugin-installer": "*",
        "parse/php-sdk": "1.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Notifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Notifications\\": "src",
            "Notifications\\Test\\": "tests",
            "Notifications\\Test\\Fixture\\": "tests/Fixture"
        }
    }
}